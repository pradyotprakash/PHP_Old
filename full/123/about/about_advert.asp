xmlhttp.send("ad_email=" + ad_email + "&ad_name=" + escape(ad_name) + "&ad_company=" + escape(ad_company) + "&ad_desc=" + escape(ad_desc));
hideAdform();
document.getElementById("ad_sent").style.display="block";
}

</script>
