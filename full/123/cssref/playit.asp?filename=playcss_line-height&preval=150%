	function click_position(obj)
	{
	change_position(obj.value)
	}
	
	function change_position(val)
	{
	
		var s="demoDIV";
		
		document.getElementById(s).style.lineHeight=val;
		
	var x="line-height:<span id='enlargecssprop'>" + val + "</span>";
	var y="###CSSPROP###;<br>";
	var z=y.replace("###CSSPROP###",x);
	
