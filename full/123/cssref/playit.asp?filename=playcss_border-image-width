	function click_position(obj)
	{
	change_position(obj.value)
	}
	
	function change_position(val)
	{
	
		var s="demoDIV";
		
			s="main";
		
		document.getElementById(s).style.borderImageWidth=val;
		
	var x="border-image-width:<span id='enlargecssprop'>" + val + "</span>";
	var y="div#main<br>		{<br>		background-color:lightgrey;<br>		border:30px solid transparent;<br>		border-image: url('border.png');<br>		border-image-slice: 30;<br>		###CSSPROP###;<br>		border-image-outset: 0;<br>		border-image-repeat: round;<br>		}<br>";
	var z=y.replace("###CSSPROP###",x);
	
		document.getElementById("styleDIV").innerHTML=z;
	
	}
	
