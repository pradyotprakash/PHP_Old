	}

	div.myease {
		animation:mymove 5s infinite;
		/* Safari and Chrome */
		-webkit-animation:mymove 5s infinite;
		-webkit-animation-timing-function:ease;	
	}
	div.myease-in {
		animation:mymove 5s infinite;
		/* Safari and Chrome */
		-webkit-animation:mymove 5s infinite;
		-webkit-animation-timing-function:ease-in;	
	}
	div.myease-out {
		animation:mymove 5s infinite;
		/* Safari and Chrome */
		-webkit-animation:mymove 5s infinite;
		-webkit-animation-timing-function:ease-out;	
	}
